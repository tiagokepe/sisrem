<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.cf.entity.Intencao" table="intencao">
		<id name="codigo">
			<column name="int_cod" not-null="true" />
			<generator class="identity" />
		</id>
		<property name="destino" column="int_destino" />
		<property name="dataInscricao" column="int_dataInscricao" />
		<property name="area" column="int_area" />
		<property name="justificativa" column="int_justificativa" />
		<property name="siape" column="int_siape" />
		<property name="origem" column="int_origem"/>
		<property name="admissao" column="int_admissao" />
		<property name="nascimento" column="int_nascimento" />
		<property name="fase" column="int_fase" />
		<property name="colocacao" column="int_colocacao" />
		<property name="email" column="int_email" />
		<property name="cargo" column="int_cargo" />
	</class>
</hibernate-mapping>

