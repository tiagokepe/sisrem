<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.cf.entity.Duvida" table="duvida">
		<id name="codigo">
			<column name="duv_cod" not-null="true" />
			<generator class="identity" />
		</id>
		<property name="enunciado" column="duv_enun" not-null="true"/>
		<property name="descricao" column="duv_desc" not-null="true"/>
	</class>
</hibernate-mapping>